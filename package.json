{
  "name": "webpack-express-elm-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for running a Webpack workflow with Elm in Node express",
  "main": "server/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickbalestra/webpack-express-elm-boilerplate.git"
  },
  "keywords": [
    "express",
    "webpack",
    "elm",
    "node"
  ],
  "author": "Nick Balestra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickbalestra/webpack-express-elm-boilerplate/issues"
  },
  "homepage": "https://github.com/nickbalestra/webpack-express-elm-boilerplate",
  "scripts": {
    "pre-commit-msg": "echo JavaScript Standard Style Check... && exit 0",
    "test": "standard server/**/*.js src/**/*.js",
    "start": "node server/main",
    "prod": "NODE_ENV=production PORT=3000 node server/main",
    "postinstall": "elm-package install",
    "build": "rimraf dist && NODE_ENV=production webpack --progress --profile --colors"
  },
  "pre-commit": [
    "pre-commit-msg",
    "test"
  ],
  "dependencies": {
    "express": "^4.13.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.19.0",
    "elm": "^0.17.1",
    "elm-hot-loader": "^0.3.4",
    "elm-webpack-loader": "^3.0.6",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.6.1",
    "json-loader": "^0.5.3",
    "node-sass": "^3.10.0",
    "postcss-loader": "^0.6.0",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.0.2",
    "standard": "^8.1.0",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0",
    "webpack-merge": "^0.14.1"
  },
  "engine": "node 6.4.0"
}
